{
  "name": "essai-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/functions/*.js",
  "scripts": {
    "build": "npm run prisma:generate && tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "dev": "npm run clean && npm run build && func start",
    "test": "echo \"No tests yet...\"",
    "prisma:generate": "prisma generate",
    "prisma:pull": "printf 'generator client {\\n  provider = \"prisma-client-js\"\\n}\\n\\ndatasource db {\\n  provider = \"sqlserver\"\\n}\\n' > prisma/schema.prisma && prisma db pull",
    "wix:collection": "ts-node scripts/get-wix-collection.ts",
    "generate-jwt": "ts-node scripts/generate-jwt.ts"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@prisma/adapter-mssql": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@wix/data": "^1.0.376",
    "@wix/sdk": "^1.21.2",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.3",
    "lru-cache": "^11.2.4",
    "mssql": "^11.0.1",
    "prisma": "^7.2.0"
  },
  "devDependencies": {
    "@prisma/config": "^7.2.0",
    "@types/mssql": "^9.1.9",
    "@types/node": "18.x",
    "azure-functions-core-tools": "^4.x",
    "nodemon": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
